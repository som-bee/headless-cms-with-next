{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "EPiServer": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "urls": "http://*:8000/;https://*:8001/;",
  "AllowedHosts": "*",
  "EPiServer": {
    "CmsUI": {
      "Upload": {
        "FileSizeLimit": 104857600
      }
    },
    "Find": {
      "DefaultIndex": "sombee_findtestreact",
      "ServiceUrl": "https://demo04.find.episerver.net/bDbXO8rMlU3cHTbAXHIKCkneP25dldAm/",
      "TrackingSanitizerEnabled": true,
      "TrackingTimeout": 30000
    }
   
  },
  "Foundation": {
    "Certificates": {
      "UseDevelopmentCertificate": true
    }
  },
  "NodeJsMiddleware": {
    "Disabled": "true",
    "Port": 3000,
    "FrontendPath": "..\\frontend",
    "AutoStart": "false"
  },
  "ConnectionStrings": {
    "EPiServerDB": "Server=localhost;Database=FoundationHeadlessCms;Integrated Security=True;TrustServerCertificate=True"
  }
}
